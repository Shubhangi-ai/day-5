def ReplaceElements(arr, n): 
  
   
    max_ele = arr[0] 
    arr[0] = -1
  
    for i in range(1, n): 
  
      
        if (max_ele > arr[i]): 
            arr[i] = max_ele 
  
       
        elif (max_ele <= arr[i]): 
            temp = arr[i] 
            arr[i] = max_ele 
            max_ele = temp 
  

 
